<%- include("components/dashboard/head"); %>


<body>
<%- include("components/dashboard/navbar"); %>
  
<div class="container-fluid">
  <div class="row">
    <%- include("components/dashboard/sidebar"); %> 

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Dashboard</h1>
      </div>
      <h2>Score Dashboard</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>User</th>
              <th>Email</th>
              <th>point</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(var user_game of user_game) { %>
            <tr>
              <td><%= user_game.username %></td>
              <td><%= user_game.email %></td>
              <td>0</td>
              <td>
                <a class="btn btn-info" href="/dashboard-edit?id=<%= user_game.id %>" role="button">EDIT</a>
                <a class="btn btn-danger" href="/dashboard-delete?id=<%= user_game.id %>" role="button">Delete</a>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>


    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
      <script>window.jQuery || document.write('<script src="../assets/js/vendor/jquery.slim.min.js"><\/script>')</script><script src="js/bootstrap.bundle.min.js"></script>

      
        <script src="https://cdn.jsdelivr.net/npm/feather-icons@4.28.0/dist/feather.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
        <script src="js/dashboard.js"></script>
  </body>
</html>
